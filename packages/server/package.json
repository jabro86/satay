    "pg": "7.11.0",
    "randomstring": "1.1.5",
    "rate-limit-redis": "1.6.0",
    "reflect-metadata": "0.1.13",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "typeorm": "0.2.18",
    "uuid": "3.3.2",
    "wait-on": "3.2.0",
    "yup": "0.27.0"
