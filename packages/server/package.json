  "dependencies": {
    "@gql2ts/from-schema": "1.10.1",
    "@satay/common": "0.1.0",
    "@sendgrid/mail": "6.4.0",
    "axios": "0.19.0",
    "bcryptjs": "2.4.3",
    "connect-redis": "3.4.1",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "express-rate-limit": "5.0.0",
