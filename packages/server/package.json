    "@types/express-session": "1.15.13",
    "@types/faker": "4.1.5",
    "@types/glob": "7.1.1",
    "@types/graphql": "14.2.1",
    "@types/ioredis": "4.0.12",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.10",
    "@types/node-fetch": "2.3.7",
    "@types/randomstring": "^1.1.6",
