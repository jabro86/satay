    "nodemon": "1.19.1",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.5"
